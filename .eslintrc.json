{
  "extends": ["eslint:recommended", "next"],
  "rules": {
    "no-console": "warn",
    "react/react-in-jsx-scope": "off",
    "sort-imports": "warn",
    "no-undef": "off",
    "no-unused-vars": "warn"
  },
  "plugins": ["simple-import-sort"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  }
}
